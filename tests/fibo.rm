mov #0, r0
mov #1, r1
mov #10000000, r3
mov #0, r5
mov #5, r6
loop :
rcopy r1, r2
add r0, r1
store r1, @storage
jeq r5, r6, @prnw
incr r5
cont : 
print @storage
printc @sp
rcopy r2, r0
jlt r1, r3, @loop
halt
prnw :
printc @nline
mov #1, r5
jmp @cont
storage : const #0

[ This is basically a nasty workaround,
 since printc now only prints a byte,
 we put the ascii equivalent of 'space'
 to the upper 8 bits, and fill the lower
 24 bits with 0 ]

sp : const #536870912

[ Same for newline ]

nline : const #167772160
